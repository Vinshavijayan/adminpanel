<h1>User Edit</h1>
{{#each user}}

<form action="/admin/editsave" method="POST">
<div class="form-group">
     <label for="name">Name</label>
     <input type="text" class="form-control" id="name" name="name" value="{{this.name}}" >
         </div>
   <div class="form-group">
    <label for="email">Email</label>
     <input type="email" class="form-control" id="email" name="email" value="{{this.email}}" readonly>
         </div>
    <div class="form-group">
         <label for="password">Password</label>
         <input type="text" class="form-control" id="password" name="password" value="{{this.password}}" >
                    </div>
    {{!-- <input type="submit" value=""> --}}

    <button type="submit">Edit</button>
</form>
{{/each}}



